{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/work-collab/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/work-collab/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/work-collab",
  "projects": {
    "work-collab": {
      "type": "application",
      "root": "apps/work-collab",
      "entryFile": "main",
      "sourceRoot": "apps/work-collab/src",
      "compilerOptions": {
        "tsConfigPath": "apps/work-collab/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "bootstrap": {
      "type": "library",
      "root": "libs/bootstrap",
      "entryFile": "index",
      "sourceRoot": "libs/bootstrap/src",
      "compilerOptions": {
        "tsConfigPath": "libs/bootstrap/tsconfig.lib.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "error": {
      "type": "library",
      "root": "libs/error",
      "entryFile": "index",
      "sourceRoot": "libs/error/src",
      "compilerOptions": {
        "tsConfigPath": "libs/error/tsconfig.lib.json"
      }
    }
  }
}